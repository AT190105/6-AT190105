#include<stdio.h>
	int main(){
		float a;
		printf("nhap a:");
		scanf("%f",&a);
		float b;
		printf("nhap b:");
		scanf("%f",&b);
		float c;
		printf("nhap c:");
		scanf("%f",&c);
		float d;
		printf("nhap d:");
		scanf("%f",&d);
		float e;
		printf("nhap e:");
		scanf("%f",&e);
		float f;
		printf("nhap f:");
		scanf("%f",&f);
		if((a/d)!=(b/e)){
			float y=(f*a-d*c)/(e*a-d*b);
			float x=(c-b*y)/a;
			printf("x=%f",x);
			printf("\ny=%f",y);
		}
        if((a/d)==(b/e)){
			if((b/e)!=(c/f)){
				if((a/d)!=(c/f)){
					printf("he phuong trinh vo nghiem");
				}
			}
		}
        if((a/d)==(b/e)){
			if((b/e)==(c/f)){
				if((a/d)==(c/f)){
					printf("he phuong trinh vo so nghiem");
				}
			}
		}

    return 0;
	}
